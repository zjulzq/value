<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zju.lzq.entity">

	<class name="Name" table="NAME_TABLE">
		<!-- for mysql -->
		<id name="id" column="ID">
			<generator class="increment"/>
		</id>
		<!-- for Oracle -->
		<!-- 
		<id name="id" column="ID">
			<generator class="native">
				<param name="sequence">NAME_SEQ</param>
			</generator>
		</id>
		-->

		<property name="firstName" column="FIRST_NAME" length="100"></property>
		<property name="familyName" column="FAMILY_NAME" length="100"></property>
		<property name="prcDate" column="PRC_DATE" type="date"></property>
	</class>

</hibernate-mapping>