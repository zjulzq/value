<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zju.lzq.entity">

	<class name="Contract" table="CONTRACT">
		<!-- for mysql -->
		<id name="id" column="ID">
			<generator class="increment"/>
		</id>
		<!-- for Oracle -->
		<!--  
		<id name="id" column="ID">
			<generator class="native">
				<param name="sequence">CONTRACT_SEQ</param>
			</generator>
		</id>
		-->
		<property name="sdate" column="SDATE" type="date"></property>
		<property name="edate" column="EDATE" type="date"></property>
		<property name="prcDate" column="PRC_DATE" type="date"></property>
	</class>

</hibernate-mapping>